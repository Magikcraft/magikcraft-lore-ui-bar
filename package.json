    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "homepage": "https://github.com/jwulf/magikcraft-lore-ui-bar#readme",
  "devDependencies": {
    "@types/jest": "^20.0.5",
    "husky": "^0.14.3",
